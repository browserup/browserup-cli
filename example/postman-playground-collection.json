			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"    pm.expect(pm.response.text()).to.include(\"foo\");",
							"});",
							"",
							"pm.test(\"Result contains 'foo' toy\", function () {",
							"    const responseJson = pm.response.json();",
							"    const filteredResponse = responseJson.filter(item => item.name === 'foo');",
							"",
							"    pm.expect(filteredResponse.length).to.be.greaterThan(0);",
							"    const expectedId = 1;",
							"    const expectedName = \"foo\";",
							"    const expectedDescription = \"bar\";",
							"",
							"    const itemWithFooName = filteredResponse[0];",
							"    pm.expect(itemWithFooName).to.have.property('id', expectedId);",
							"    pm.expect(itemWithFooName).to.have.property('name', expectedName);",
							"    pm.expect(itemWithFooName).to.have.property('description', expectedDescription);",
							"",
							"}); "
						],
						"type": "text/javascript"
					}
				}
