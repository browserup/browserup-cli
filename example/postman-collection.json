	"item": [
		{
			"name": "Test postman-echo",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"  pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response contains json with data key\", function () {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson.data).to.be.a('string');",
							"});",
							"",
							"pm.test(\"Response data string is the same as in request\", function () {",
							"    const responseJson = pm.response.json();",
							"    pm.expect(responseJson.data).to.eql(pm.request.body.raw);",
							"});",
							" "
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "text/plain"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "Duis posuere augue vel cursus pharetra. In luctus a ex nec pretium..."
				},
				"url": {
					"raw": "https://postman-echo.com/post",
					"protocol": "https",
					"host": [
						"postman-echo",
						"com"
					],
					"path": [
						"post"
					]
				}
			},
			"response": []
		},
