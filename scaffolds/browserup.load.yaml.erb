scenario:
  name: <%= defined?(scenario_name) ? scenario_name : 'GiveMeABetterName' %>
  total_users: <%= defined?(total_users) ? total_users : '5' %>
  profiles:
<% example_arr.each_with_index do |example, i| -%>
<%= "\s\s\s\s-#{example.collect {|k,v| ("\s#{k}: \"#{v}\"")}.join("\n\s\s\s\s\s")}" %>
<% end -%>
  stop_after: 15m
  ramp:
    - ramp_to: 100%
      over: 5m

reports:
  - name: 'UrlResponseTime'
    title: 'URL 90th Response Time (SLA)'
    type: 'line'
    cards:
      - metrics:
          - metric: 'urls.response_ms.avg'
            check:
              '<': 2000

settings:
  cluster_type: local
